<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SpringMvc" namespace="SpringMvc.Models.POCO">
  <class name="QuantityMap" table="QuantityMap">

    <id name="Id" column="Id" type="Int64">
      <generator class="foreign">
        <param name="property">BookType</param>
      </generator>
    </id>

    <property name="Quantity" column="Quantity" type="int"/>
    <one-to-one name="UserAccount" class="UserAccount" constrained="true"/>
    
  </class>
</hibernate-mapping>